{
  "name": "@ant-design/x-markdown",
  "version": "2.0.0",
  "scripts": {
    "compile": "father build",
    "tsc": "tsc --noEmit",
    "lint": "npm run tsc && npm run lint:script && npm run lint:md",
    "lint:md": "remark . -f -q",
    "lint:script": "biome lint",
    "pretest": "npm run version",
    "predist": "npm run version && npm run plugin:meta",
    "test": "jest --config .jest.js --no-cache --collect-coverage",
    "coverage": "jest --config .jest.js --no-cache --collect-coverage --coverage",
    "plugin:meta": "tsx scripts/generate-plugin-meta.ts",
    "prestart": "npm run version && npm run plugin:meta",
    "precompile": "npm run prestart",
    "version": "tsx scripts/generate-version.ts",
    "test:dekko": "tsx ./tests/dekko/index.test.ts"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "plugins",
    "themes"
  ],
  "license": "MIT",
  "description": "placeholder for @ant-design/x-markdown",
  "dependencies": {
    "@ant-design/fast-color": "^2.0.6",
    "@ant-design/icons": "^5.6.1",
    "classnames": "^2.5.1",
    "html-react-parser": "^5.2.5",
    "katex": "^0.16.22",
    "lodash.throttle": "^4.1.1",
    "marked": "^15.0.12",
    "mermaid": "^11.8.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@umijs/mako": "^0.11.10",
    "github-markdown-css": "^5.8.1",
    "marked-footnote": "^1.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "repository": "https://github.com/ant-design/x.git"
}
